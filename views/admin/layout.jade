        a.tipR(href='#', title='Hide Left Sidebar')
          span.icon12.minia-icon-layout
      // Sidebar background
      #sidebarbg
      // Sidebar content
      #sidebar
        .shortcuts
          ul
            li
              a.tip(href='support.html', title='Support section')
                span.icon24.icomoon-icon-support
            li
              a.tip(href='#', title='Database backup')
                span.icon24.icomoon-icon-database
            li
              a.tip(href='charts.html', title='Sales statistics')
                span.icon24.icomoon-icon-pie-2
            li
              a.tip(href='#', title='Write post')
                span.icon24.icomoon-icon-pencil
        // End search
        .sidenav

      // Body content
      #content.clearfix
        .contentwrapper
          // Content wrapper
          .heading
            h3= title
            .resBtnSearch
              a(href='#')
                span.icon16.icomoon-icon-search-3
            .search
              form#searchform(action='search.html')
                input#tipue_search_input.top-search(type='text', placeholder='Search here ...')
                input#tipue_search_button.search-btn(type='submit', value='')
            // End search
            ul.breadcrumb

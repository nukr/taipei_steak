    // loading animation
    #qLoverlay
    #qLbar
    #header
      .navbar
        .navbar-inner
          .container-fluid
            a.brand(href='/admin/dashboard')
              | 台北牛排
              span.slogan admin
            .nav-no-collapse
              ul.nav
                li.active
                  a(href='/admin/dashboard')
                    span.icon16.icomoon-icon-screen-2
                    | Dashboard
                li.dropdown
                  a.dropdown-toggle(href='#', data-toggle='dropdown')
                    span.icon16.icomoon-icon-cog
                    | Settings
                    b.caret
                  ul.dropdown-menu
                    li.menu
                      ul
                        li
                          a(href='#')
                            span.icon16.icomoon-icon-equalizer
                            | Site config
                        li
                          a(href='#')
                            span.icon16.icomoon-icon-wrench
                            | Plugins
                        li
                          a(href='#')
                            span.icon16.icomoon-icon-picture-2
                            | Themes
                li.dropdown
                  a.dropdown-toggle(href='#', data-toggle='dropdown')
                    span.icon16.icomoon-icon-mail-3
                    | Messages
